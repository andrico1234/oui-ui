{
    "name": "@oui-ui/checkbox",
    "publishConfig": {
        "access": "public"
    },
    "author": "Andrico Karoulla",
    "scripts": {
        "cem:analyze": "custom-elements-manifest analyze --outdir '.' --globs \"lib/**/Checkbox.js\""
    },
    "homepage": "https://github.com/andrico1234/oui-ui",
    "version": "0.2.5",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/andrico1234/oui-ui.git",
        "directory": "packages/checkbox"
    },
    "main": "lib/index.js",
    "module": "lib/index.js",
    "typings": "lib/index.d.ts",
    "keywords": [
        "web-components",
        "open-ui",
        "ui",
        "checkbox"
    ],
    "files": [
        "lib/*",
        "custom-elements.json"
    ],
    "engines": {
        "node": ">=12"
    },
    "size-limit": [
        {
            "path": "dist/web.cjs.production.min.js",
